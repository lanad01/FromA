<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	
	<bean id="apple" class="sample2.Apple">
		<property name="name" value="사과"></property>
	</bean>
	<bean id="banana" class="sample2.Banana">
		<property name="name" value="바나나"></property>
	</bean>
	<bean id="kim" class="sample2.KimChangMin">
		<property name="name" value="KimchangMin"></property>
		<property name="item" ref="apple"></property>
	</bean>
	<bean id="kwon" class="sample2.KwonSangwoo">
		<property name="name" value="권상우"></property>
		<property name="item" ref="banana"></property>
	</bean>
</beans>
