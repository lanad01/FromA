<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	
	<bean id="outputter" class="sample1.FileOutputter">
		<property name="filePath" value="out.txt"></property>
	</bean>
	<bean id="messageBean" class="sample1.MessageBeanImpl">
		<constructor-arg value="김창민"></constructor-arg>
		<property name="greeting" value="잘생겼네요~"></property>
		<property name="outputter" ref="outputter"></property>
	</bean>
</beans>
